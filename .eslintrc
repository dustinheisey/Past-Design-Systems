{
    "root": true,
    "extends": [
        "eslint:recommended",
        "plugin:prettier/recommended",
        "plugin:storybook/recommended",
        "plugin:tailwindcss/recommended"
    ],
    "env": {
        "browser": true,
        "es2021": true
    },
    "parserOptions": {
        "ecmaVersion": 2021,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "plugins": ["import"]
}
