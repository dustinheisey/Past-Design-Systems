{
  "name": "design-system",
  "private": true,
  "version": "1.0.0",
  "description": "Marketing website design system",
  "repository": {
    "type": "git",
    "url": "https://github.com/dustinheisey/eleventy-design-system.git"
  },
  "scripts": {
    "lint:js": "npx eslint **/*.js",
    "lint:css": "cd styles & npx stylelint \"**/*.css\" --fix",
    "lint": "npm run lint:js & npm run lint:css",
    "format": "prettier \"**/*.{cjs,html,js,json,md,njk,css}\" --ignore-path ./.eslintignore --write",
    "storybook": "storybook dev -c stories/.storybook",
    "styles": "postcss styles/style.css -o styles/style.min.css --watch",
    "start": "cd sites/portfolio & rollup -c rollup.config.js & npx @11ty/eleventy --serve --quiet --incremental",
    "build": "npm run format & cd sites/portfolio & rollup -c rollup.config.js & npx @11ty/eleventy",
    "ally": "cd sites/portfolio & a11y-sitechecker --config=ally.config.json -T=1000"
  },
  "author": {
    "name": "Dustin Heisey"
  },
  "devDependencies": {
    "@11ty/eleventy": "^2.0.1",
    "@11ty/eleventy-img": "^3.1.0",
    "@11ty/eleventy-navigation": "^0.3.5",
    "@11ty/eleventy-plugin-inclusive-language": "^1.0.3",
    "@11ty/eleventy-plugin-syntaxhighlight": "^5.0.0",
    "@rollup/plugin-terser": "^0.4.3",
    "@storybook/addon-a11y": "^7.0.7",
    "@storybook/addon-essentials": "^7.0.7",
    "@storybook/addon-styling": "^1.0.3",
    "@storybook/blocks": "^7.0.7",
    "@storybook/cli": "^7.0.15",
    "@storybook/html": "^7.0.15",
    "@storybook/html-webpack5": "^7.0.15",
    "a11y-sitechecker": "^6.0.1",
    "addon-screen-reader": "^1.8.6",
    "autoprefixer": "^10.4.14",
    "chromatic": "^6.17.3",
    "cssnano": "^6.0.1",
    "eleventy-plugin-gen-favicons": "^1.1.2",
    "eleventy-plugin-html-validate": "^0.0.5",
    "eleventy-plugin-pwa-v2": "^1.0.0",
    "eslint": "^8.41.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^15.7.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-storybook": "^0.6.12",
    "html-minifier": "^4.0.0",
    "luxon": "^3.3.0",
    "markdown-it": "^13.0.1",
    "markdown-it-anchor": "^8.6.7",
    "postcss": "^8.4.23",
    "postcss-cli": "^10.1.0",
    "postcss-easy-import": "^4.0.0",
    "postcss-preset-env": "^8.4.1",
    "postcss-reporter": "^7.0.5",
    "prettier": "^2.8.8",
    "purgecss": "^5.0.0",
    "purgecss-from-html": "^6.0.0-alpha.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "simple-nunjucks-loader": "^3.2.0",
    "storybook": "^7.0.15",
    "storybook-addon-rem": "^1.3.0",
    "stylelint-config-standard": "^33.0.0",
    "util": "^0.12.5"
  },
  "engines": {
    "node": ">=12.0.0",
    "npm": ">=6.0.0"
  }
}
