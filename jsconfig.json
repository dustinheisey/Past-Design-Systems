{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "$app/*": [".svelte/dev/runtime/app/*", ".svelte/build/runtime/app/*"],
      "$styles": ["src/styles/index.css"],
      "$components/*": ["src/components/*"],
      "$layout/*": ["src/components/layout/*"],
      "$sections/*": ["src/sections/*"]
    }
  },
  "include": ["src/**/*.js", "src/**/*.svelte"]
}
